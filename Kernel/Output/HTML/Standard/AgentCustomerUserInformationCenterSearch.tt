<div class="InnerContent">
    <form action="[% Env("CGIHandle") %]" method="post" id="AgentCustomerUserInformationCenterSearchForm">
        <input type="hidden" name="Action" value="[% Env("Action") %]"/>
        <input type="hidden" name="Subaction" value="Redirect"/>
        <fieldset class="TableLike">
            <label>[% Translate("Customer") | html %]:</label>
            <div class="Field">
                <input type="text" id="AgentCustomerUserInformationCenterSearchCustomerID" class="W90pc" />
            </div>
            <div class="Clear"></div>
            <label>[% Translate("Customer user") | html %]:</label>
            <div class="Field">
                <input type="text" id="AgentCustomerUserInformationCenterSearchCustomerUser" class="W90pc" />
            </div>
            <div class="Clear"></div>
        </fieldset>
    </form>
</div>
<script type="text/javascript">//<![CDATA[
Core.Agent.CustomerUserInformationCenterSearch.Init();
//]]></script>
